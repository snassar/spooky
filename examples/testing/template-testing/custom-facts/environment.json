{
  "environment": "testing",
  "deployment_type": "docker",
  "app_version": "1.0.0",
  "database": {
    "type": "postgresql",
    "version": "15",
    "port": 5432,
    "host": "db-server"
  },
  "cache": {
    "type": "redis",
    "version": "7",
    "port": 6379,
    "host": "cache-server"
  },
  "monitoring": {
    "enabled": true,
    "prometheus_port": 9090,
    "grafana_port": 3000,
    "alertmanager_port": 9093
  },
  "load_balancer": {
    "type": "nginx",
    "port": 80,
    "ssl_port": 443
  },
  "backup": {
    "enabled": true,
    "retention_days": 30,
    "schedule": "0 2 * * *"
  }
} 