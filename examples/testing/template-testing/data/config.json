{
  "application": {
    "name": "Test Application",
    "version": "1.0.0",
    "description": "A test application for unit testing",
    "author": "Test Team",
    "license": "MIT"
  },
  "features": {
    "authentication": true,
    "authorization": true,
    "logging": true,
    "monitoring": true,
    "backup": true
  },
  "settings": {
    "debug": false,
    "maintenance_mode": false,
    "cache_enabled": true,
    "session_timeout": 3600,
    "max_upload_size": "64MB"
  },
  "integrations": {
    "database": {
      "type": "postgresql",
      "host": "db-server",
      "port": 5432,
      "name": "app_db"
    },
    "cache": {
      "type": "redis",
      "host": "cache-server",
      "port": 6379
    },
    "email": {
      "type": "smtp",
      "host": "smtp.test.local",
      "port": 587,
      "encryption": "tls"
    }
  }
} 